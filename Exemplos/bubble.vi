int teste[]={1,7,8,4,5,2,3,9,6}
int i
int j
int aux
for(i = 0 ;i < i - 1 ;i=i+1)
{
    for(j = 0 ;j < 10 - i - 1 ;j=j+1)
    {
        if(teste[j] > teste[j + 1])
        {
            aux= teste[j]
            teste[j] = teste[j + 1]
            teste[j + 1] = aux
        }
    }
}
maduro("\n List Sorted in Ascending Order : ")
for(i = 0;i < 10 ;i=i+1)
{
    maduro(teste[i])
}
maduro("\n")
